        "eslint-plugin-mocha": "^8.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prefer-arrow": "^1.2.2",
        "eslint-plugin-promise": "^4.2.1"
    },
    "eslintConfig": {
        "root": true,
